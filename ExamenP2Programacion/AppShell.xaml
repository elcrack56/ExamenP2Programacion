<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ExamenP2Programacion.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ExamenP2Programacion"
    
    Title="ExamenP2Programacion">

    <TabBar>
        <Tab Title="PaginaPrincipal" Icon="dotnet_bot.png">
            <ShellContent
                    Title="Home"
                    ContentTemplate="{DataTemplate local:MainPage}"
                    Route="MainPage" /> 
        </Tab>
        <Tab Title="Pestaña1" Icon="dotnet_bot.png">
            <ShellContent
                 Title="Pestaña1"
                 ContentTemplate="{DataTemplate local:Pestaña1Recarga}"
                 Route="Pestaña1Recarga" />
        </Tab>
        <Tab Title="Pestaña2" Icon="dotnet_bot.png">
            <ShellContent
                 Title="Pestaña2"
                 ContentTemplate="{DataTemplate local:Pestaña2GridNombre}"
                 Route="Pestaña2GridNombre" />
        </Tab>
    </TabBar>
</Shell>
